amends "..."

deployments {
  ["server"] {
    spec {
      template {
        spec {
          containers {
            new {
              name = "server"
              image = "ghcr.io/keithhand/khand.dev:dev"
              imagePullPolicy = "Always"
              env {
                new {
                  name = "LOG_LEVEL"
                  value = "warn"
                }
              }
              ports {
                new {
                  name = "ui"
                  containerPort = 8080
                }
              }
            }
          }
        }
      }
    }
  }
}
