amends "..."

ingresses {
  ["server"] {
    spec {
      tls {
        new {
          hosts {
            "khand.dev"
          }
          secretName = "khand-dev-cert"
        }
      }
      rules {
        new {
          host = "khand.dev"
          http {
            paths {
              new {
                path = "/"
                pathType = "Prefix"
                backend {
                  service {
                    name = "server"
                    port {
                      name = "ui"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
