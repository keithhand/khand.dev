amends "template.pkl"

import "label.pkl"

deployments {
  default {
    metadata {
      namespace = "khand-dev"
    }
    spec {
      selector {
        matchLabels = label.matchModule(module)
      }
      template {
        metadata {
          labels {
            ...label.matchModule(module)
          }
        }
      }
    }
  }
}
